var PSKweb=PSKweb||{};PSKweb.UserClock=function(){var n=function(){PSKweb.User.timezone.Display!=null&&($("#localTime").parent().attr("data-original-title",$("#localTime").parent().attr("data-original-title")+" "+PSKweb.User.timezone.Display.split(") ")[0]+")"),$("#localTime").parent().attr("title",$("#localTime").parent().attr("title")+" "+PSKweb.User.timezone.Display.split(") ")[0]+")"));setInterval(i,1e3)},t=function(){$("#sessionTimeElapsed").length>0&&setInterval(r,1e3)},i=function(){var n=PSKweb.TimeHelper.toPlayerTimezone(new Date);$("#localTime").text(("0"+n.getHours()).slice(-2)+":"+("0"+n.getMinutes()).slice(-2)+":"+("0"+n.getSeconds()).slice(-2))},r=function(){var e=PSKweb.TimeHelper.toPlayerTimezone(new Date(localStorage.getItem("SessionStartTime"))),u=PSKweb.TimeHelper.toPlayerTimezone(new Date),n,i,r,f,t,o;u<e&&u.setDate(u.getDate()+1);n=u-e;i=Math.floor(n/36e5);n-=i*36e5;r=Math.floor(n/6e4);n-=r*6e4;f=Math.floor(n/1e3);$("#sessionTimeElapsed").text(("0"+i).slice(-2)+":"+("0"+r).slice(-2)+":"+("0"+f).slice(-2));t=$("#sessionTimeElapsed").parent();o=t.attr("aria-describedby")!==undefined;i>0&&r===0&&f<30&&!o?(t.tooltip({trigger:"manual"}),t.tooltip("show")):i>0&&r===0&&f===30&&(t.tooltip({trigger:"hover focus"}),t.tooltip("hide"))},u=function(){setTimeout(function(){$("#clock-separator").text("|")},1e3)},f=function(){n();t();u()};return{init:f}}()