PSKweb.Loading.addLoadingFunction(function(){$(".region-filter").on("click",function(n){n.preventDefault();$(this).toggleClass("active");$(".region-list").toggleClass("active")});$(".region-list .region").on("click",function(){var f=$(this),r=f.data("region-id"),n=$("#regions-table tr").not(".thead"),u="[data-region-id="+r+"]",t,i;r!=-1?(n.not(u).hide(),t=n.filter(u).show()):t=n.show();i="";t.each(function(){var t=$(this),n=t.data("letter"),r=i!==n;i=n;t.find(".letter").text(r?n:"")});$(".region-filter, .region-list").removeClass("active");$(".region-filter").html($(this).html())})},"Pages/BetShops.js");PSKweb.Loading.addLoadingFunction(function(){$(".main-instruction-title-color.instructions-chapter.collapsing-header").on("click",function(){var n=$(this),t,i;n.toggleClass("collapsed");n.hasClass("collapsed")&&(t=$(this).attr("id"),i=$("[data-sport="+t+"]").filter('.in:not(".collapsing-header")'),i.each(function(n,t){var i=$(t);i.collapse("hide")}))})},"Pages/BettingInstructions.js")