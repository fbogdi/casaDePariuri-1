PSKweb.Loading.addLoadingFunction(function(){var n,t,r,e,o,u,f;i=1;n=$("#LottoOfferTable tr.lotto-row");n.length?(n.hide(),$("#LottoOfferTable tr:last").show(),n.slice(0,20*i).show(),n.length<=20&&$("#LottoOfferTable tr:last").hide()):$("#AllLotto tr:last").hide();$("#showMore").click(function(){i++;i*20>=n.length?(n.slice(0,n.length).show(),$("#LottoOfferTable tr:last").hide()):(n.slice(0,20*i+1).show(),n.length<=20*i+1&&$("#LottoOfferTable tr:last").hide())});PSKweb.Betslip.ControllerLotto.init();t=$("div.lotto-event-container[data-numbers-for-event-id]");t.length==1?PSKweb.Betslip.ControllerLotto.setContest(t.data("numbers-for-event-id")):(r=$("div.lotto-event-container[data-numbers-for-event-id]:not(:hidden)").data("numbers-for-event-id"),r!=null&&PSKweb.Betslip.ControllerLotto.setContest(r),t.each(function(n,t){$(t).data("numbers-for-event-id")!=r&&PSKweb.Betslip.ControllerLotto.getLottoRules($(t).data("numbers-for-event-id"))}));e=function(){$("div.lotto-picker-container div.lotto-numbers-container div.lotto-numbers div.lotto-number").removeClass("active");PSKweb.Betslip.ModelLotto.picks.each(function(n){$("div.lotto-picker-container div.lotto-numbers-container div.lotto-numbers div.lotto-number[data-pick='"+n.get("Pick")+"']").addClass("active")})};PSKweb.Betslip.ModelLotto.picks.on("change add remove reset",e);o=function(n){var t=$(n.currentTarget),i=t.data("pick"),r=t.closest("[data-numbers-for-event-id]").data("numbers-for-event-id");PSKweb.Betslip.ControllerLotto.togglePick(i,r)};$("div.lotto-picker-container div.lotto-numbers-container div.lotto-numbers div.lotto-number[data-pick]").click(o);$(".lotto-header-pick").on("click",function(){var n=$(this),t=n.data("event-id");$(".lotto-header-pick").removeClass("active");$("[data-numbers-for-event-id]").hide();$("[data-numbers-for-event-id="+t+"]").show();n.addClass("active");u();PSKweb.Betslip.ControllerLotto.setContest(t)});u=function(){var t=$(".lotto-header-pick.active").data("lotto-code"),n={IL102090:"blue",IWFL1020:"green",GKL2080:"orange"}[t]||"blue";f("gradient-transparent",n);f("gradient-opaque",n)};f=function(n,t){$("."+n+"-blue, ."+n+"-green, ."+n+"-orange").removeClass(n+"-blue "+n+"-green "+n+"-orange").addClass(n+"-"+t)};u()},"Lotto/LottoView.js")